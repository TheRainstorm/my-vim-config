### virtual machine
###
# ssh VM-Ubuntu18.0-server-longxin
Host ubuntu20
  HostName 127.0.0.1
  Port 2201
  User yfy
  IdentityFile ~/.ssh/id_rsa

Host ubuntu20-docker
  HostName 127.0.0.1
  Port 2202
  User root
  IdentityFile ~/.ssh/id_rsa

