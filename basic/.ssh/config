### main
###
# git@github.com:TheRainstorm/repo
Host github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa

# git@gitee.com:yuanfuyan/repo
Host gitee.com
  HostName gitee.com
  User git
  IdentityFile ~/.ssh/id_rsa

# git@y0fy:y0fy/repo
Host y0fy
  HostName github.com
  User git
  IdentityFile ~/.ssh/y0fy_rsa

